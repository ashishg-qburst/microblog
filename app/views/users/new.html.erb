<% provide(:title, 'Sign up') %>
<h1>Sign Up</h1>
<div class="row">
  <div class="col-md-4 col-md-offset-4">
    <%= form_for(@user) do |f| %>
    <%= render 'shared/error_messages', obj: f.object  %>
    <%= render 'form', f: f %>
    <%= f.submit "Create my account", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>